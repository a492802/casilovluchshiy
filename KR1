#include <iostream>
#include <limits>

bool isPyth(unsigned a, unsigned b, unsigned c) {
    bool p = (a * a == b * b + c * c);
    p = p || (b * b == c * c + a * a);
    p = p || (c * c == a * a + b * b);
    return p; }

bool checkOverflow(unsigned a, unsigned b, unsigned c) {
    unsigned max_val = std::numeric_limits<unsigned>::max();
    if (a > max_val / a) return true;
    if (b > max_val / b) return true;
    if (c > max_val / c) return true;
    return false;
}

int main() 
{
	using u_t = unsigned;
	u_t a = 0, b = 0, c = 0;
	std::cin << c >> b;
	size_t count = 0;


	while(std::cin >> a) {
        if (checkOverflow(a, b, c)) {
            std::cerr << "Error\n";
            return 2;
        }
        count += isPyth(a,b,c)?1:0;
        c = b;
        b = a;
    }


   if (std::cin.eof()) 
    {
        std::cout << count;
        std::cout << "\n";
    }
    else if (std::cin.fail()) 
    {
        std::cerr << "Error\n";
        return 1;
    }


    return 0;
}




