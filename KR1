#include <iostream>



bool isPyth(unsigned a, unsigned b, unsigned c) 
{
    bool p = (a * a == b * b + c * c);
    p = p || (b * b == c * c + a * a);
    p = p || (c * c == a * a + b * b);
    return p; 
}



bool checkOverflow(unsigned a, unsigned b, unsigned c) 
{
    unsigned a2 = a * a;
    unsigned b2 = b * b; 
    unsigned c2 = c * c;
    if (a > 0 && a2 / a != a) return true;
    if (b > 0 && b2 / b != b) return true;
    if (c > 0 && c2 / c != c) return true;
    return false; 
}



int main() 
{
	using u_t = unsigned;
	u_t a = 0, b = 0, c = 0;
	std::cin << c >> b;
	size_t count = 0;

	while(std::cin >> a) {
        if (checkOverflow(a, b, c)) {
            std::cerr << "Error\n";
            return 2; }
        count += isPyth(a,b,c)?1:0;
        c = b;
        b = a;}

   if (std::cin.eof()) {
        std::cout << count;
        std::cout << "\n";}

    else if (std::cin.fail()) {
        std::cerr << "Error\n";
        return 1;}

    return 0;
}




